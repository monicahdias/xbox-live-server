generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Profile {
  id       String @id @default(uuid())
  title    String @unique
  imageUrl String

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model User {
  id       String  @id @default(uuid())
  name     String
  email    String  @unique
  password String  @unique
  cpf      Int     @unique
  isAdmin  Boolean

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
